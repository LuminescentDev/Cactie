  "dependencies": {
    "@prisma/client": "5.13.0",
    "body-parser": "1.20.2",
    "diff": "5.2.0",
    "discord.js": "14.15.2",
    "dotenv": "16.4.5",
    "express": "4.18.2",
    "hastebin": "0.2.1",
    "ms": "2.1.3",
    "node-cron": "3.0.3",
    "prisma": "5.13.0",
    "uuid": "9.0.1",
    "winston": "3.13.0"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.8",
    "utf-8-validate": "6.0.3"
  },
