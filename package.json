{
  "name": "cactie",
  "version": "9.2.1",
  "description": "A bot that does stuff ig",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saboooor/Cactie.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "git pull && pnpm i && ts-node --transpileOnly src/index.ts",
