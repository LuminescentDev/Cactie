    "lint:fix": "eslint . --fix",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate start"
  },
  "main": "src/index.ts",
  "keywords": [
    "discord",
    "bot",
    "discordjs"
  ],
