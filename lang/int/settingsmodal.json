{
    "prefix": {
        "type": "text",
        "min": 1,
        "max": 10,
        "style": "Short",
        "placeholder": "-",
        "required": true
    },
    "leavemessage": {
        "type": "text",
        "min": 0,
        "max": 2000,
        "style": "Paragraph",
        "placeholder": "Keep empty to disable.\nVars: \"{USER MENTION}\" \"{USER TAG}\"",
        "required": false
    },
    "joinmessage": {
        "type": "text",
        "min": 0,
        "max": 2000,
        "style": "Paragraph",
        "placeholder": "Keep empty to disable.\nVars: \"{USER MENTION}\" \"{USER TAG}\"",
        "required": false
    },
    "maxppsize": {
        "type": "text",
        "min": 1,
        "max": 2,
        "style": "Short",
        "placeholder": "Must be a *number* between 1 and 75.",
        "required": true
    },
    "msgshortener": {
        "type": "text",
        "min": 1,
        "max": 4,
        "style": "Short",
        "placeholder": "Must be a *number*.\nSet to 0 to disable.",
        "required": true
    },
    "disabledcmds": {
        "type": "text",
        "min": 0,
        "max": 2000,
        "style": "Paragraph",
        "placeholder": "Commands to disable.\nSeparate with commas.",
        "required": false
    },
    "reactions": { "type": "bool" },
    "suggestthreads": { "type": "bool" },
    "tickets": {
        "type": "select",
        "options": ["buttons", "reactions", "false"]
    },
    "language": {
        "type": "select",
        "options": []
    },
    "suggestionchannel": {
        "type": "id",
        "from": "channels",
        "func": "isText"
    },
    "pollchannel": {
        "type": "id",
        "from": "channels",
        "func": "isText"
    },
    "logchannel": {
        "type": "id",
        "from": "channels",
        "func": "isText"
    },
    "ticketcategory": {
        "type": "id",
        "from": "channels",
        "func": "isCategory"
    },
    "supportrole": {
        "type": "id",
        "from": "roles"
    },
    "ticketmention": {
        "type": "id",
        "from": "roles",
        "additional": ["here", "everyone"]
    },
    "mutecmd": {
        "type": "id",
        "from": "roles",
        "additional": ["timeout"]
    },
    "adminrole": {
        "type": "id",
        "from": "roles"
    },
    "djrole": {
        "type": "id",
        "from": "roles"
    }
}